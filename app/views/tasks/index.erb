<h3>All of <%= current_user.username%>'s Tasks:</h3>

<% current_user.tasks.each do |task| %>
  <%= task.name %><br />
  <a href="/tasks/<%=task.id%>/edit">Edit Task</a>
  <br />
  <br />
<% end %>
<button onclick="goBack()">Go Back</button>

<script>
function goBack() {
  window.history.back();
}
</script>
